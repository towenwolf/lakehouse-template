{
  "dashboard": {
    "title": "Lakehouse Data Product Dashboard",
    "tags": ["lakehouse", "data-engineering"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Data Ingestion Rate",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(lakehouse_ingestion_records_total[5m])",
            "legendFormat": "{{layer}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Pipeline Success Rate",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(lakehouse_pipeline_success_total[1h])) / sum(rate(lakehouse_pipeline_total[1h])) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Data Quality Scores",
        "type": "gauge",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "lakehouse_data_quality_score",
            "legendFormat": "{{dataset}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Processing Time by Layer",
        "type": "heatmap",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(lakehouse_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "{{layer}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(lakehouse_errors_total[5m])",
            "legendFormat": "{{error_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      }
    ]
  }
}
